<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="iOS Web Cloud mobile testing Suite">
    <parameter name="cloudName" value="EPAM Mobile CLoud"/>
    <parameter name="appType" value="web"/>
    <parameter name="deviceName" value="IPHONE 12,5"/>
    <parameter name="platformName" value="iOS"/>
    <parameter name="browserName" value="Safari"/>
    <test name="iOS Cloud Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
