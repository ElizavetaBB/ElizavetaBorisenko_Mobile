<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="iOS Native Cloud mobile testing Suite">
    <parameter name="cloudName" value="EPAM Mobile CLoud"/>
    <parameter name="appType" value="native"/>
    <parameter name="deviceName" value="IPHONE 12,1"/>
    <parameter name="platformName" value="iOS"/>
    <parameter name="bundleId" value="com.epam.EPAMTestApp.ii"/>
    <test name="iOS Cloud Native tests">
        <groups>
            <run>
                <include name="native"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
