<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Android Native Cloud mobile testing Suite">
    <parameter name="cloudName" value="EPAM Mobile CLoud"/>
    <parameter name="appType" value="native"/>
    <parameter name="deviceName" value="HMD GLOBAL  7 plus"/>
    <parameter name="platformName" value="Android"/>
    <parameter name="appPackage" value="platkovsky.alexey.epamtestapp"/>
    <parameter name="appActivity" value="activities.LoginActivity"/>
    <test name="Android Cloud Native tests">
        <groups>
            <run>
                <include name="native"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
