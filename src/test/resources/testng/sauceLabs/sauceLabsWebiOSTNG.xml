<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="iOS Web Sauce Labs mobile testing Suite">
    <parameter name="cloudName" value="Sauce Labs"/>
    <parameter name="appType" value="web"/>
    <parameter name="deviceName" value="iPhone_XR_free"/>
    <parameter name="platformName" value="iOS"/>
    <parameter name="platformVersion" value="15.4.1"/>
    <parameter name="browserName" value="Safari"/>
    <test name="iOS Sauce Labs Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
